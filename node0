#include<iostream>
#include<cstring>
using namespace std;
struct node {
	int value;
	string* farm = new string[value];
	struct node* next;
};
int main() {
	node* head = NULL;
	node* p = new node;
	p->value = 2;
	int x = p->value;
	if (head == NULL) {
		head = p;
		p->next = NULL;
	}
	while (x != 0) {
		for (int i = 1; i <= x; i++) {
			cin >> p->farm[i];
		}
		node* q = new node;
		p->next = q;
		q->next = NULL;
		cin >> q->value;
		x = q->value;
	}
	return 0;
}
